<html>
	<head>
		<link rel="stylesheet" type="text/css" href="/styles.css" />
		<title><%= title %></title>
	</head>
	<body>
		<%- include('navbar', { categories: categories }) %>

		<div class="main">
			<h1><%= title %></h1>

			<ul>
				<% for (let i = 0; i < subcats.length; i++) { %>
					<li class="mainList">
						<a href="/list/<%= category %>/<%= subcats[i].name %>">
							<%= subcats[i].name %> 
						</a>
					</li>
				<% } %>
			</ul>
		</div>
	</body>
</html>
